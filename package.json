{
  "name": "@healwrap/url-editor",
  "displayName": "URL Editor",
  "version": "0.0.1",
  "description": "URL编辑器，提升开发效率",
  "author": "pepedd@qq.com",
  "privacy_policy": "https://github.com/healwrap/url-editor/blob/master/PRIVACY.md",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.23.0",
    "@ant-design/icons": "^5.6.1",
    "@plasmohq/storage": "^1.15.0",
    "@types/qrcode.react": "^3.0.0",
    "@webext-core/messaging": "^2.2.0",
    "antd": "^5.24.1",
    "plasmo": "0.90.3",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "urijs": "^1.19.11",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/urijs": "^1.19.25",
    "@types/webextension-polyfill": "^0.12.3",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "prettier": "3.2.4",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.85.0",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "storage",
      "webRequest",
      "tabs",
      "activeTab"
    ],
    "commands": {
      "open-popup": {
        "suggested_key": {
          "default": "Ctrl+Shift+1",
          "mac": "Command+Shift+1",
          "linux": "Ctrl+Shift+1",
          "chromeos": "Ctrl+Shift+1",
          "windows": "Ctrl+Shift+1"
        },
        "description": "Open the popup",
        "global": true
      }
    }
  }
}
